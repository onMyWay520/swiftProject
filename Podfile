source 'https://mirrors.tuna.tsinghua.edu.cn/git/CocoaPods/Specs.git'
platform :ios,'9.0'
target 'swiftProject' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
	  pod 'MJRefresh', :git => 'https://gitee.com/wyc520/MJRefresh.git'
    pod 'SDWebImage', :git => 'https://gitee.com/wyc520/SDWebImage.git', :tag => '5.2.2'
    pod 'SnapKit'
    pod 'RxSwift'
    pod 'HandyJSON' , :git => 'https://gitee.com/wyc520/HandyJSON.git'
    pod 'SwiftyJSON'
    pod 'Alamofire'
    pod 'FMDB'
    pod 'LLCycleScrollView'
    pod 'RxDataSources'
    pod 'RxAlamofire'
    pod 'ObjectMapper'
    pod 'LookinServer', :git => 'https://gitee.com/wyc520/LookinServer.git', :configurations => ['Debug']
end
